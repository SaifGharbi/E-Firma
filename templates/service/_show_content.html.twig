<div class="container">
    <h1>Service</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ service.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ service.nom }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ service.description }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ service.prix }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ service.type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_service_index') }}">back to list</a>
    {% if is_granted('ROLE_VETERINAIRE') or is_granted('ROLE_EXPERT') %}
        <a href="{{ path('app_service_edit', {'id': service.id}) }}">edit</a>
        {{ include('service/_delete_form.html.twig') }}
    {% endif %}
</div> 